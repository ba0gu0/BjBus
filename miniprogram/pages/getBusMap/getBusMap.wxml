<view class='text'>

  <text>{{ lineName }}</text>
  <map class='map'
    id="myMap"
    longitude="{{ longitude }}"
    latitude="{{ latitude }}"
    markers="{{ markers }}"
    polyline="{{ polyline }}"
    include-points="{{ includePoints }}"
    show-location="true"
    show-compass="true"
    ></map>
</view>